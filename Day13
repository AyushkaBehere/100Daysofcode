//LOOPS
//While Loop
import java.util.*;

public class Loops {
    public static void main(String args[]) {
        int counter = 0;
        while (counter < 10) {
            System.out.println("Hello World");
            counter++;
        }

        System.out.println("Printed HW 10x");

    }
}


//Print nos. from 1 to 10
public class WhileEven {

    public static void main(String[] args) {
        int i=0;
        int num=0;
        System.out.println("Even numbers");
        while(i<=10)
        {
            System.out.println(num);
            i+=1;
            num+=2;
        }
    }
}
