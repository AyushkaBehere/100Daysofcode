// 16. Primes in Range
import java.util.*;

public class functions {
    public static boolean isPrime(int n){
    public static void primesInRange(int n) {
        for (int i = 2; i <= n; i++) {
            if (isPrime(i)) {
                System.out.print(i + " ");
            }
        }
    }}

    public static void main(String args[]) {
        System.out.println(isPrime(16));
    }
}
